{
  "name": "@elfo404/grid",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Giordano Ricci <me@giordanoricci.com>",
  "repository": "github:Elfo404/gio",
  "homepage": "https://elfo404.github.io/gio/?path=/story/grid-readme--page",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev": "npm run clean && tsc --watch",
    "build": "npm run clean && tsc",
    "//": "TODO: remove all other files on prepublishOnly",
    "prepublishOnly": "npm run build",
    "limit": "npm run build && size-limit",
    "precommit": "lint-staged --quiet"
  },
  "devDependencies": {
    "@emotion/react": "^11.1.5",
    "@elfo404/eslint-config-react": "^0.0.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@size-limit/preset-small-lib": "^4.10.2",
    "@size-limit/time": "^4.10.2",
    "eslint": "7.23.0",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "10.5.4",
    "prettier-plugin-import-sort": "^0.0.6",
    "size-limit": "^4.10.2",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "@emotion/react": "^11.1.5",
    "react": "^17.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "module"
    }
  },
  "size-limit": [
    {
      "path": "./dist/*.js",
      "limit": "800 B"
    }
  ]
}
